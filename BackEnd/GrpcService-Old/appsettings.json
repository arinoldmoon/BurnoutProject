{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "SQLiteConnection": "Data source=Data/oven.db"
  },
  "PLCConfig": {
    "PortName": "/dev/ttyUSB0",
    "BaudRate": 19200,
    "Delay1": 100,
    "Delay2": 1000,
    "Delay3":10
  }
}
